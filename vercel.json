{
    "version": 2,
    "name": "novasphere-awwwards",
    "builds": [
        {
            "src": "app.mjs",
            "use": "@vercel/node",
            "config": {
                "nodeVersion": "22.x"
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
